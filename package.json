{
  "$schema": "https://raw.githubusercontent.com/sourcegraph/sourcegraph/master/shared/src/schema/extension.schema.json",
  "name": "eslint",
  "description": "Shows problems reported by ESLint",
  "icon": "https://raw.githubusercontent.com/eslint/website/master/assets/img/logo.svg",
  "publisher": "sourcegraph",
  "activationEvents": [
    "*"
  ],
  "wip": true,
  "categories": [
    "Code analysis",
    "Reports and stats",
    "Linters",
    "Insights"
  ],
  "tags": [],
  "contributes": {
    "actions": [],
    "menus": {
      "editor/title": [],
      "commandPalette": []
    },
    "configuration": {
      "type": "object",
      "properties": {
        "eslint.insight.repository": {
          "description": "The repository to query on global insights",
          "type": "string"
        },
        "eslint.insight.step": {
          "description": "The time duration between two data points on the X axis.",
          "type": "object",
          "properties": {
            "years": {
              "type": "number"
            },
            "months": {
              "type": "number"
            },
            "weeks": {
              "type": "number"
            },
            "days": {
              "type": "number"
            },
            "hours": {
              "type": "number"
            },
            "minutes": {
              "type": "number"
            },
            "seconds": {
              "type": "number"
            }
          }
        }
      }
    }
  },
  "version": "0.0.0-DEVELOPMENT",
  "license": "Apache-2.0",
  "main": "dist/eslint.js",
  "scripts": {
    "eslint": "eslint 'src/**/*.ts'",
    "typecheck": "tsc -p tsconfig.json",
    "build": "parcel build --out-file dist/eslint.js src/eslint.ts",
    "symlink-package": "mkdirp dist && lnfs ./package.json ./dist/package.json",
    "serve": "yarn run symlink-package && parcel serve --no-hmr --out-file dist/eslint.js src/eslint.ts",
    "watch:typecheck": "tsc -p tsconfig.json -w",
    "watch:build": "tsc -p tsconfig.dist.json -w",
    "sourcegraph:prepublish": "yarn run typecheck && yarn run build"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Edge versions",
    "last 1 Safari versions"
  ],
  "devDependencies": {
    "@sourcegraph/eslint-config": "^0.19.2",
    "@sourcegraph/tsconfig": "^4.0.1",
    "@types/lodash": "^4.14.155",
    "eslint": "^7.2.0",
    "lnfs-cli": "^2.1.0",
    "mkdirp": "^1.0.4",
    "parcel-bundler": "^1.12.4",
    "sourcegraph": "^24.5.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "date-fns": "^2.14.0",
    "lodash": "^4.17.15",
    "rxjs": "^6.5.5",
    "tagged-template-noop": "^2.1.1"
  }
}
